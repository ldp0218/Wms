<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="5dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="5dp">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="10dp"
        android:text="流程卡号" />

    <TextView
        android:id="@+id/textView_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/textView5"
        android:textColor="@android:color/black"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView_namelabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView_no"
        android:layout_marginBottom="5dp"
        android:layout_marginEnd="10dp"
        android:labelFor="@+id/textView_date"
        android:text="成品名称" />

    <TextView
        android:id="@+id/textView_name"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_no"
        android:layout_toEndOf="@+id/textView_namelabel"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView_modellabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_no"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_toStartOf="@+id/textView_model"
        android:text="型号" />

    <TextView
        android:id="@+id/textView_model"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView_no"
        android:textColor="@android:color/black" />

</RelativeLayout>